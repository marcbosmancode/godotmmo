[application]

config/name="Godot_client"
run/main_scene="res://Title.tscn"
config/icon="res://icon.png"

[autoload]

NetworkController="*res://NetworkController.gd"
Global="*res://Global.gd"
SceneChanger="*res://SceneChanger.tscn"
ChatBox="*res://UI/ChatBox.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://UI/Cursors/cursorx2.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

